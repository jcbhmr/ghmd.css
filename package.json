{
  "unpkg": "dist/index.css",
  "exports": {
    ".": "./dist/index.css",
    "./dark": "./dist/dark.css",
    "./light": "./dist/light.css"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:index.css": "postcss src/index.scss dist/index.css",
    "build:dark.css": "postcss src/dark.scss -o dist/dark.css",
    "build:light.css": "postcss src/light.scss -o dist/light.css"
  },
  "files": ["dist"]
}
